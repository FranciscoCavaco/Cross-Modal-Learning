{
  "AdamOptimizer": {
    "args": {
      "lr": 0.001,
      "weight_decay": 0.0
    },
    "name": "Adam",
    "scheduler_args": {
      "factor": 0.1,
      "mode": "min",
      "patience": 5,
      "threshold": 0.01,
      "threshold_mode": "abs"
    }
  },
  "CRNNWordModel": {
    "args": {
      "audio_encoder": {
        "in_dim": 64,
        "out_dim": 300,
        "up_sampling": true
      },
      "text_encoder": {
        "word_embedding": {
          "embed_dim": 300,
          "pretrained": true,
          "trainable": false
        }
      }
    },
    "name": "CRNNWordModel"
  },
  "PANNWordModel": {
    "args": {
      "audio_encoder": {
        "classes_num": 300,
        "freeze_base": true,
        "pretrain_path": "pretrain/Cnn14_16k.pth"
      },
      "text_encoder": {
        "word_embedding": {
          "embed_dim": 300,
          "pretrained": true,
          "trainable": false
        }
      }
    },
    "name": "PANNWordModel"
  },
  "TripletRankingLoss": {
    "args": {
      "margin": 1.0
    },
    "name": "TripletRankingLoss"
  },
  "audio": {
    "h5py_name": "audio_logmel"
  },
  "captions": {
    "caption_prefix": "clotho_captions_",
    "captions_dir": "Captions",
    "json_dir": "SavedData"
  },
  "data": {
    "dataset_dir": "Clotho",
    "emb_file": "word2vec_emb.pkl",
    "hdf5_dir": "SavedData",
    "hdf5_file": "audio_logmel.hdf5",
    "pickle_dir": "SavedData",
    "splits": {
      "test": "evaluation",
      "train": "development",
      "val": "validation"
    }
  },
  "experiment": "cross_modal_cw",
  "output_path": "ray_tune",
  "ray_conf": {
    "init_args": {
      "_temp_dir": "/tmp/ray",
      "ignore_reinit_error": true,
      "local_mode": false,
      "num_cpus": 1,
      "num_gpus": 1
    },
    "reporter": "CLIReporter",
    "search_alg": null,
    "search_space": {},
    "stopper_args": {
      "grace_period": 60,
      "metric": "validation_loss",
      "metric_threshold": null,
      "mode": "min",
      "num_results": 10,
      "std": 0.01
    },
    "trial_scheduler": null,
    "trial_stopper": "TrialPlateauStopper"
  },
  "training": {
    "algorithm": {
      "batch_size": 32,
      "criterion": "TripletRankingLoss",
      "epochs": 150,
      "optimizer": "AdamOptimizer"
    },
    "model": "PANNWordModel"
  }
}